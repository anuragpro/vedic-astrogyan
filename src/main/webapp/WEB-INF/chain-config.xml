<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
http://www.springframework.org/schema/util
http://www.springframework.org/schema/util/spring-util-2.5.xsd">
	<bean name="pingPongChain" class="org.apache.commons.chain.impl.ChainBase">
		<constructor-arg>
			<util:list>
				<ref bean="pingCommand" />
				<ref bean="pongCommand" />
			</util:list>
		</constructor-arg>
	</bean>
	<bean name="pingCommand" class="com.vedic.astro.chain.PingCommand">
		<constructor-arg value="ping" />
	</bean>
	<bean name="pongCommand" class="com.vedic.astro.chain.PongCommand">
		<constructor-arg value="pong" />
	</bean>
	<bean name="yogasAndDoshasChain" class="org.apache.commons.chain.impl.ChainBase">
		<constructor-arg>
			<util:list>
				<ref bean="kaalSarpaYogFinder" />
				<ref bean="manglikDoshaFinder" />
			</util:list>
		</constructor-arg>
	</bean>
	<bean id="chainRunner" class="com.vedic.astro.chain.ChainRunner" />
</beans>